{
    "name": "nodejs_skeleton",
    "version": "1.0.0",
    "description": "NodeJS Skeleton",
    "license": "UNLICENSED",
    "private": true,
    "author": "Gwenael Walet",
    "watch": {
        "build:server": {
            "patterns": [
                "src",
                "i18n"
            ],
            "extensions": "js,ts,json",
            "quiet": true
        }
    },
    "scripts": {
        "mocha": "mocha --recursive './dist/server/test/*.js'",
        "tsc": "tsc",
        "clean:server-dist": "rimraf dist/server",
        "build-dev": "npm-watch",
        "build:server": "tsc",
        "start-dev": "supervisor -w dist -t -- dist",
        "start": "npm-run-all clean:* tsc -p build-dev start-dev",
        "build": "npm-run-all clean:* -p build:*",
        "test": "npm-run-all clean:server-dist tsc mocha"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.2",
        "@types/cookie-parser": "1.4.3",
        "@types/csurf": "1.11.2",
        "@types/express": "4.17.13",
        "@types/express-form-data": "^2.0.5",
        "@types/express-session": "1.17.4",
        "@types/fs-extra": "9.0.13",
        "@types/lodash": "4.14.182",
        "@types/mocha": "9.1.1",
        "@types/node-cron": "3.0.1",
        "@types/passport": "1.0.7",
        "@types/passport.socketio": "3.7.7",
        "@types/redis": "4.0.11",
        "@types/socket.io": "3.0.2",
        "@types/socket.io-redis": "3.0.0",
        "@types/string": "0.0.31",
        "@types/uid-safe": "2.1.2",
        "@types/underscore": "^1.11.15",
        "@types/winston": "2.4.4",
        "@types/zxcvbn": "4.4.1",
        "mocha": "10.0.0",
        "npm-run-all": "4.1.5",
        "npm-watch": "0.11.0",
        "rimraf": "3.0.2",
        "supervisor": "0.12.0",
        "typescript": "4.7.2"
    },
    "dependencies": {
        "body-parser": "1.20.0",
        "connect-session-sequelize": "7.1.3",
        "cookie-parser": "1.4.6",
        "cors": "^2.8.5",
        "csurf": "1.11.0",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "express-form-data": "2.0.18",
        "express-limiter": "1.6.1",
        "express-permission-rules": "2.0.10",
        "express-session": "1.17.3",
        "fs-extra": "10.1.0",
        "generate-password": "1.7.0",
        "got": "11.8.2",
        "helmet": "5.1.0",
        "lodash": "4.17.21",
        "md5": "^2.3.0",
        "md5-file": "5.0.0",
        "moment": "2.29.3",
        "mustache": "4.2.0",
        "mysql2": "2.3.3",
        "node-cron": "3.0.0",
        "passport": "0.6.0",
        "passport-http-bearer": "1.0.1",
        "passport.socketio": "3.7.0",
        "redis": "4.1.0",
        "reflect-metadata": "0.1.13",
        "sequelize": "6.20.0",
