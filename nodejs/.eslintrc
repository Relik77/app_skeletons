{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "rules": {
    "array-bracket-spacing": [2],
    "block-scoped-var": 1,
    "brace-style": [1, "1tbs"],
    "camelcase": ["off"],
    "comma-dangle": ["error", "never"],
    "comma-spacing": 2,
    "computed-property-spacing": [2, "never"],
    "consistent-return": [
      "warn",
      {
        "treatUndefinedAsUnspecified": false
      }
    ],
    "dot-notation": [
      "error",
      {
        "allowKeywords": true
      }
    ],
    "eol-last": 1,
    "eqeqeq": ["off", "smart"],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1,
        "ArrayExpression": "first",
        "FunctionExpression": {
          "body": 1,
          "parameters": "first"
        },
        "CallExpression": {
          "arguments": 1
        },
        "ObjectExpression": "first"
      }
    ],
    "key-spacing": [
      "error", {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
      }
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "max-depth": [1, 4],
    "max-params": ["warn", 5],
    "new-cap": [
      2,
      {
        "newIsCap": true,
        "capIsNew": false
      }
    ],
    "no-alert": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-console": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 1,
    "no-empty-character-class": 2,
    "no-labels": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [
      "warn",
      "all",
      {
        "nestedBinaryExpressions": false
      }
    ],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-irregular-whitespace": 2,
    "no-label-var": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-multi-spaces": [
      "error",
      {
        "exceptions": {
          "Property": true,
          "VariableDeclarator": true,
          "ImportDeclaration": true
        }
      }
    ],
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 1,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-shadow": [
      "warn",
      {
        "allow": ["done", "callback", "resolve", "reject", "cb", "next", "err", "result"]
      }
    ],
    "no-spaced-func": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [
      "warn",
      {
        "args": "none",
        "caughtErrors": "none"
      }
    ],
    "no-use-before-define": [1, "nofunc"],
    "no-with": 2,
    "object-curly-spacing": [2, "never"],
    "quote-props": [1, "as-needed"],
    "quotes": "off",
    "radix": 2,
    "semi": 2,
    "space-before-function-paren": [
      "warn",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "never"
      }
    ],
    "space-infix-ops": 2,
    "keyword-spacing": 2,
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": 2
  }
}